<div class="content-wrapper">
  <div class="content-header">
    <div class="container-fluid">
      <div class="col-sm-12">
        <h1 class="m-0 text-dark">
          New User
        </h1>
      </div>
    </div>
  </div>
  <div class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-body">
              <%= bootstrap_form_for @user, layout: :horizontal, label_col: 'col-md-12', control_col: 'col-md-12' do |f| %>
                <%= render partial: 'form', locals: {f: f} %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
